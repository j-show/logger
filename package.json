{
  "name": "@jshow/logger",
  "version": "1.0.3",
  "license": "MIT",
  "homepage": "https://github.com/j-show/logger",
  "repository": {
    "type": "git",
    "url": "https://github.com/j-show/logger.git"
  },
  "bugs": {
    "url": "https://github.com/j-show/logger/issues"
  },
  "keywords": [
    "logger",
    "jshow",
    "jshow-logger"
  ],
  "files": [
    "dist",
    "README.md",
    "README_CN.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "engines": {
    "pnpm": ">=10"
  },
  "scripts": {
    "prepare": "husky",
    "clean": "rm -rf ./dist && rm -rf ./out",
    "build": "pnpm clean && vite build",
    "test": "vitest --run",
    "example:basic": "ts-node ./examples/basic.ts",
    "example:advanced": "ts-node ./examples/advanced.ts",
    "example:config": "ts-node ./examples/configuration.ts",
    "example:color": "ts-node ./examples/color.ts",
    "example:real": "ts-node ./examples/real-world.ts"
  },
  "devDependencies": {
    "@types/node": "^22",
    "eslint": "^9",
    "eslint-config-jshow": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-jshow": "latest",
    "eslint-plugin-prettier": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "prettier": ">=3",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5",
    "typescript-eslint": "^8",
    "vite": "^7",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4"
  }
}
